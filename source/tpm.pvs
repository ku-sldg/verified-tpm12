  %% Wrap new key k with parent key
  TPM_CreateWrapKey(parent,keyInfo:(tpmKey?)) : State = 
     modifyOutput(
	  (LAMBDA (s:tpmAbsState) :
	      executeCom(s,ABS_CreateWrapKey(parent,keyInfo))),
          (LAMBDA (s:tpmAbsState) :
